#!/usr/bin/env python

import sys, os, shutil, string;

testname=sys.argv[1]
compopts=sys.argv[4]
dist = 'default'

s = string.rfind(compopts, 'distString')

print compopts

if s != -1:
    opt = compopts[s+11:len(compopts)]
    dist = opt.split()[0]

print dist

goodfn=testname+'.'+dist+'.good'
if os.path.isfile(goodfn):
    shutil.copy(goodfn, testname+'.good')
else:
    shutil.copy(testname+'.default.good', testname+'.good')

