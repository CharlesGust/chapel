#!/bin/csh -f
set testname = $1
set outfile  = $2
set tmpfile  = $outfile.prediff.tmp

echo "Outfile: " $outfile
echo "tmpFile: " $tmpfile

# Replace threads IDS with "ID"
sed 's/[0-9]*\.[0-9]* seconds./NUM seconds./g' $outfile > $tmpfile
sed 's/[0-9]*\.[0-9]* Gflops\/s/NUM Gflops\/s/g' $tmpfile > $outfile

rm $tmpfile

