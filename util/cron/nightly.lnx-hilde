#!/usr/bin/tcsh

#
# Test Nanos with CHPL_COMM=none
#
source ~/.chapelcshrc-cron
setenv CHPL_TASKS nanox
$CHPL_HOME/util/cron/nightly-tasks -cron -examples >&! ~/tmp/nightly.nanox.out

source ~/.chapelcshrc-cron-gasnet
setenv CHPL_GASNET_CFG_OPTIONS --disable-ibv
setenv GASNET_QUIET Y
$CHPL_HOME/util/cron/nightly-tasks -cron -examples >&! ~/tmp/nightly.nanox-gasnet.out

