#!/bin/tcsh
#

# run block dist regressions
source ~/.chplcron.dists.cshrc && ~/chapel/util/cron/nightly -cron -dist block >&! ~/tmp/nightly.block.out

set date = `date +%Y-%m-%d`

# run memleaks regressions
source ~/.chplcron.memleaks.cshrc && ~/chapel/util/cron/nightly -cron -examples -memleaks memleaks.$date.nightly.log >&! ~/tmp/nightly.memleaks.out
